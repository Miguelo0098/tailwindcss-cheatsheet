{
  "name": "tailwind-cheatsheet",
  "displayName": "tailwind-cheatsheet",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tailwindCheatsheet",
          "title": "tailwind cheatsheet",
          "icon": "resources/tailwind-icon.svg"
        }
      ]
    },
    "views": {
      "tailwindCheatsheet": [
        {
          "id": "tailwindCheatsheet",
          "name": "tailwind cheatsheet"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "tailwindCheatsheet",
